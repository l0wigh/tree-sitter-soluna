{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "soluna",
      "camelcase": "Soluna",
      "title": "Soluna",
      "scope": "source.soluna",
      "file-types": [
        "luna"
      ],
      "injection-regex": "^soluna$",
      "class-name": "TreeSitterSoluna",
      "highlights": "queries/highlights.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Parser for the Soluna language",
    "authors": [
      {
        "name": "L0Wigh",
        "email": "mathiotthomas@gmail.com",
        "url": "https://github.com/L0Wigh"
      }
    ],
    "links": {
      "repository": "https://github.com/L0Wigh/tree-sitter-soluna",
      "funding": ""
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "java": false,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
